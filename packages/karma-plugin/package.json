{
  "name": "@jtex/karma-plugin",
  "version": "1.0.0-alpha",
  "description": "jtex karma plugin",
  "author": "y1j2x34 <y1j2x34@qq.com>",
  "homepage": "https://github.com/y1j2x34/jtex/tree/master/packages/karma-plugin#readme",
  "license": "MIT",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "distDir": "lib",
  "keywords": [
    "jtex",
    "unit",
    "testing",
    "extension",
    "json",
    "html",
    "reporter",
    "awesome",
    "karma"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "doc-assets"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "lint": " eslint {src,__tests__,build}/**/*.ts",
    "format": "eslint {src,__tests__,build}/**/*.ts  --fix",
    "build": "rimraf ./lib && rollup -c ./build/rollup/rollup.config.js",
    "debug": "npm run build && npm run start:debug",
    "start:debug": "cross-env TS_NODE_PROJECT=\"./tsconfig.json\" karma start __tests__/karma.conf.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/chrome": "0.0.124",
    "@types/jest": "^26.0.20",
    "@types/karma": "^5.0.0",
    "@types/karma-coverage-istanbul-reporter": "^2.1.0",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "codecov": "^3.8.0",
    "cross-env": "^7.0.2",
    "del": "^6.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "jest-html-reporter": "^3.3.0",
    "karma": "^6.0.3",
    "mkdirp": "^1.0.4",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.0.3",
    "txon": "^1.0.0-rc.7",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@jtex/common": "^1.0.0",
    "@jtex/karma-json2html": "1.0.0"
  }
}
